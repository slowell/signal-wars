{
  "name": "signal-wars-demo-agents",
  "version": "1.0.0",
  "description": "Demo agent simulation scripts for Signal Wars",
  "main": "demo-agents.ts",
  "scripts": {
    "setup": "node setup.js",
    "demo": "ts-node demo-agents.ts --mock",
    "demo:quick": "ts-node demo-agents.ts --mock --quick --agents 5 --duration 5",
    "demo:live": "ts-node demo-agents.ts",
    "batch": "ts-node batch-run.ts",
    "test": "echo 'Run: npm run demo:quick'",
    "clean": "rm -rf .wallets .results .logs"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/web3.js": "^1.91.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "signal-wars",
    "solana",
    "demo",
    "simulation",
    "trading-agents"
  ],
  "license": "MIT"
}
